{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://empty-pumas-call.loca.lt/api/$1" },
    { "source": "/auth/(.*)", "destination": "https://empty-pumas-call.loca.lt/auth/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
