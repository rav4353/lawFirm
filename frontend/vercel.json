{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://cute-apples-think.loca.lt/api/$1" },
    { "source": "/api", "destination": "https://cute-apples-think.loca.lt/api" },
    { "source": "/((?!api).*)", "destination": "/index.html" }
  ]
}
