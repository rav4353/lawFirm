  "liveNow": false,
  "panels": [
    {
      "title": "Requests Per Minute (RPM)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(veritas_api_request_duration_seconds_count[1m])) * 60",
          "legendFormat": "Total Requests"
        }
      ]
    },
    {
      "title": "Compliance Pass/Fail Rates",
      "type": "state-timeline",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum by (framework, status) (rate(veritas_compliance_audit_total[5m]))",
          "legendFormat": "{{framework}} - {{status}}"
        }
      ]
    },
    {
      "title": "Latencies (P95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(veritas_api_request_duration_seconds_bucket[5m])))",
          "legendFormat": "API Latency"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(veritas_ai_inference_duration_seconds_bucket[5m])))",
          "legendFormat": "AI Inference Latency"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(veritas_opa_decision_duration_seconds_bucket[5m])))",
          "legendFormat": "OPA Decision Latency"
        }
      ]
    }
  ],
